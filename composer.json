{
  "name": "michel/revision_using_composer",
  "description": "revision composer",
  "license": "MIT",
  "scripts": {
    "tests": "./vendor/bin/phpunit tests --colors always",
    "start": "php -S localhost:4000"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "files": [
      "src/before/before.php",
      "src/controllers/homeController.php",
      "src/routers/personals/userRouter.php",
      "src/routers/personals/managerRouter.php"
    ],
    "classmap": [
      "src"
    ]
  },
  "authors": [
    {
      "name": "michel",
      "email": "mtchokos@gmail.com"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^10.2",
    "guzzlehttp/guzzle": "^7.7",
    "nyholm/symfony-bundle-test": "^2.0"
  }
}
